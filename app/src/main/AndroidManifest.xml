    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleCoordinatorActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".SwipeBehaviorExampleActivity" />
        <activity android:name=".IOActivityExample2" />
        <activity android:name=".FlexibleSpaceExampleActivity" />
        <activity
            android:name=".IOActivityExample"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".MaterialUpConceptActivity" />
        <activity android:name=".TabActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
    </application>

</manifest>